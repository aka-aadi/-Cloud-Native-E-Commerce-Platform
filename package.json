  "scripts": {
    "build": "next build",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "deploy": "./scripts/deploy-100-percent-free.sh",
    "deploy:aws": "./scripts/deploy-free-tier.sh",
    "deploy:local": "./scripts/local-development.sh",
    "dev": "next dev",
    "docker:build": "docker build -t musicmart .",
    "docker:run": "docker run -p 3000:3000 musicmart",
    "lint": "next lint",
    "monitor": "./monitor-free-usage.sh",
    "ssh": "ssh -i ~/.ssh/id_rsa ec2-user@$(terraform -chdir=terraform output -raw application_url | cut -d'/' -f3 | cut -d':' -f1)",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
