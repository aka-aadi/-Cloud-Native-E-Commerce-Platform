{
  "name": "musicmart-free-tier",
  "version": "1.0.0",
  "private": true,
  "description": "100% Free AWS E-commerce Platform",
  "keywords": [
    "aws",
    "free-tier",
    "ecommerce",
    "nextjs",
    "postgresql",
    "terraform",
    "zero-cost"
  ],
  "homepage": "https://github.com/your-username/musicmart-ecommerce#readme",
